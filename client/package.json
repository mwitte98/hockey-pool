{
  "name": "hockey-pool",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": [
    "> 0.8% in US"
  ],
  "watch": {
    "lint": {
      "patterns": "**/*",
      "extensions": "*"
    }
  },
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "lint": "sass-lint -v --max-warnings 0 && ng lint",
    "lint-fix": "ng lint --fix",
    "lint-watch": "npm-watch lint",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "test-ci": "jest --coverage --runInBand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-next.5",
    "@angular/cdk": "8.1.4",
    "@angular/common": "9.0.0-next.5",
    "@angular/compiler": "9.0.0-next.5",
    "@angular/core": "9.0.0-next.5",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "9.0.0-next.5",
    "@angular/material": "8.1.4",
    "@angular/platform-browser": "9.0.0-next.5",
    "@angular/platform-browser-dynamic": "9.0.0-next.5",
    "@angular/router": "9.0.0-next.5",
    "core-js": "3.2.1",
    "hammerjs": "2.0.8",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-next.3",
    "@angular/cli": "9.0.0-next.3",
    "@angular/compiler-cli": "9.0.0-next.5",
    "@types/node": "12.7.4",
    "@types/jest": "24.0.18",
    "codelyzer": "5.1.0",
    "jest": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "sass-lint": "1.13.1",
    "tslint": "5.16.0",
    "typescript": "3.5.3"
  }
}
