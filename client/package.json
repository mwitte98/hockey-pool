{
  "name": "hockey-pool",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": [
    ">=0%"
  ],
  "watch": {
    "lint": {
      "patterns": "**/*",
      "extensions": "*"
    }
  },
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "lint": "stylelint ./src --max-warnings 0 && ng lint --format=verbose && $(npm bin)/prettylint './**/*.*'",
    "lint-fix": "stylelint ./src --fix && ng lint --fix --format=verbose && $(npm bin)/prettylint './**/*.*' --fix",
    "lint-watch": "npm-watch lint",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "test-ci": "jest --coverage --maxWorkers=2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.12",
    "@angular/cdk": "11.2.11",
    "@angular/common": "11.2.12",
    "@angular/compiler": "11.2.12",
    "@angular/core": "11.2.12",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.12",
    "@angular/material": "11.2.11",
    "@angular/platform-browser": "11.2.12",
    "@angular/platform-browser-dynamic": "11.2.12",
    "@angular/router": "11.2.12",
    "rxjs": "6.6.3",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.11",
    "@angular/cli": "11.2.11",
    "@angular/compiler-cli": "11.2.12",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.1",
    "codelyzer": "6.0.1",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "postcss": "8.2.13",
    "prettier": "2.2.1",
    "prettylint": "1.0.0",
    "stylelint": "13.13.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "stylelint-prettier": "1.2.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.1.5"
  }
}
