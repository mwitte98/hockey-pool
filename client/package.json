{
  "name": "hockey-pool",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0 <21.0.0"
  },
  "watch": {
    "lint": {
      "patterns": "**/*",
      "extensions": "*"
    }
  },
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build-prod": "ng build -c production",
    "stylelint": "stylelint './src/**/*.scss' --max-warnings 0",
    "stylelint-fix": "stylelint './src/**/*.scss' --fix",
    "nglint": "ng lint",
    "nglint-fix": "ng lint --fix",
    "prettier": "prettier --check './**/*.*'",
    "prettier-fix": "prettier --write './**/*.*'",
    "lint": "npm run stylelint && npm run nglint && npm run prettier",
    "lint-fix": "npm run stylelint-fix && npm run nglint-fix && npm run prettier-fix",
    "lint-watch": "npm-watch lint",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "test-ci": "jest --coverage --maxWorkers=2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.1.3",
    "@angular/cdk": "18.1.3",
    "@angular/common": "18.1.3",
    "@angular/compiler": "18.1.3",
    "@angular/core": "18.1.3",
    "@angular/forms": "18.1.3",
    "@angular/material": "18.1.3",
    "@angular/platform-browser": "18.1.3",
    "@angular/platform-browser-dynamic": "18.1.3",
    "@angular/router": "18.1.3",
    "@swimlane/ngx-charts": "20.5.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-eslint/builder": "18.2.0",
    "@angular-eslint/eslint-plugin": "18.2.0",
    "@angular-eslint/eslint-plugin-template": "18.2.0",
    "@angular-eslint/schematics": "18.2.0",
    "@angular-eslint/template-parser": "18.2.0",
    "@angular/build": "18.1.3",
    "@angular/cli": "18.1.3",
    "@angular/compiler-cli": "18.1.3",
    "@types/jest": "29.5.12",
    "@types/node": "22.1.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "jest": "29.7.0",
    "jest-preset-angular": "14.2.2",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-prettier": "5.0.0",
    "typescript": "5.5.4"
  }
}
