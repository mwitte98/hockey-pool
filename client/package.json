{
  "name": "hockey-pool",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": [
    ">0.01% in US"
  ],
  "watch": {
    "lint": {
      "patterns": "**/*",
      "extensions": "*"
    }
  },
  "scripts": {
    "postinstall": "ngcc --loglevel debug --properties es2015 browser module main --first-only --async false",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "lint": "stylelint ./src --max-warnings 0 && ng lint --format=verbose && $(npm bin)/prettylint './**/*.*'",
    "lint-fix": "stylelint ./src --fix && ng lint --fix --format=verbose && $(npm bin)/prettylint './**/*.*' --fix",
    "lint-watch": "npm-watch lint",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "test-ci": "jest --coverage --runInBand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.1",
    "@angular/cdk": "10.0.0",
    "@angular/common": "10.0.1",
    "@angular/compiler": "10.0.1",
    "@angular/core": "10.0.1",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.0.1",
    "@angular/material": "10.0.0",
    "@angular/platform-browser": "10.0.1",
    "@angular/platform-browser-dynamic": "10.0.1",
    "@angular/router": "10.0.1",
    "rxjs": "6.5.5",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.0",
    "@angular/cli": "10.0.0",
    "@angular/compiler-cli": "10.0.1",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.14",
    "codelyzer": "5.2.2",
    "jest": "26.1.0",
    "jest-preset-angular": "8.2.1",
    "prettier": "2.0.5",
    "prettylint": "1.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-prettier": "1.1.2",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.9.5"
  }
}
